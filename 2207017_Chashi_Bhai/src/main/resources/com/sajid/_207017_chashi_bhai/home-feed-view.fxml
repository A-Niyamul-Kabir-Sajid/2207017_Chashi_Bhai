<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>

<VBox xmlns:fx="http://javafx.com/fxml" spacing="10" alignment="CENTER">
    <Label text="Home Feed" styleClass="header"/>
    <ListView fx:id="feedListView" prefWidth="400" prefHeight="300"/>
    <Button text="View Transaction History" onAction="#handleTransactionHistory"/>
    <Button text="View Crop Feed" onAction="#handleCropFeed"/>
</VBox>