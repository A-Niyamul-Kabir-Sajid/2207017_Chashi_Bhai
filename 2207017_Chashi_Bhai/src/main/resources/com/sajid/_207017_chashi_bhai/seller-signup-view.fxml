<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx">
    <children>
        <Label text="Seller Sign-Up" layoutX="200.0" layoutY="20.0" textAlignment="CENTER">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label text="Name:" layoutX="50.0" layoutY="80.0" />
        <TextField fx:id="nameField" layoutX="150.0" layoutY="80.0" />
        
        <Label text="Email:" layoutX="50.0" layoutY="120.0" />
        <TextField fx:id="emailField" layoutX="150.0" layoutY="120.0" />
        
        <Label text="Password:" layoutX="50.0" layoutY="160.0" />
        <PasswordField fx:id="passwordField" layoutX="150.0" layoutY="160.0" />
        
        <Label text="Phone Number:" layoutX="50.0" layoutY="200.0" />
        <TextField fx:id="phoneField" layoutX="150.0" layoutY="200.0" />
        
        <Button text="Sign Up" layoutX="250.0" layoutY="250.0" onAction="#handleSignUp" />
        <Button text="Back to Welcome" layoutX="350.0" layoutY="250.0" onAction="#handleBackToWelcome" />
    </children>
</AnchorPane>